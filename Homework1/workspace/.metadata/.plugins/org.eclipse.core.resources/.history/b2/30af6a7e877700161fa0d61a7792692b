package com.main.example.classes;

import java.util.List;

public abstract class LivingBeing
{
	private int nYearsLived;
	private List<? extends LivingBeing> ls;
	public int getnYearsLived()
	{
		return nYearsLived;
	}

	public void setnYearsLived(int nYearsLived)
	{
		this.nYearsLived = nYearsLived;
	}

}
