<body>
<script type="text/javascript">
function locate()
{
	navigator.geolocation.watchPosition(displayPosition);
	}
	function displayPosition(userposition)
	{
	document.getElementById("data").innerHTML="Longitude: "+userposition.coords.latitude+"<br>Latitude: "+userposition.coords.latitude+"<br>Accuracy?"+userposition.coords.accuracy+"<br>speed"+userposition.coords.speed;
	var mix=userposition.coords.latitude+","+userposition.coords.longitude;
	var image = "http://maps.googleapis.com/maps/api/staticmap?center="
	    +mix+"&zoom=14&size=400x300&sensor=false";
	    document.getElementById("demo1").innerHTML = "<img src='"+image+"'>";
	}

</script>
Enter city name<input type = "search" name = "googlesearch"><br>
<input type="button" value ="locate" onclick="locate()">

<p id ="data"></p>
<p id ="data"></p>

</body>
