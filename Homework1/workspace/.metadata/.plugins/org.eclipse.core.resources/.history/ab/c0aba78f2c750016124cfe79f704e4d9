function result()
{
	var marks = document.getElementById("marks");
	var result = document.getElementById("result");
	if(marks > 95)
		{
			result.innerHTML = "A+";
		}
	else if(marks <= 95 && marks > 89)
		{
			result.innerHTML = "A";
		}
	else if (marks < 89 && marks > 80)
		{
			result.innerHTML = "B";
		}
	else if(marks <  && marks > 80)
}
</script>
